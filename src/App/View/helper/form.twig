<form id="formSubmit" accept-charset="utf-8" enctype="multipart/form-data" autocomplete="off" class="form form-horizontal" method="post">

    {% if content is not empty %}
        <div class="container-fluid">
            {{ content|raw }}
        </div>
        <hr/>
        <div class="form-group">
            <div class="col-lg-12">
              <button id="botaoSubmit" type="submit" class="btn btn-primary">Save</button>
              <a onclick="history.go(-1);/*location.href = document.referrer; return false;*/" class="btn btn-default" >Cancel</a>
            </div>
        </div>
    {% endif %}
</form>